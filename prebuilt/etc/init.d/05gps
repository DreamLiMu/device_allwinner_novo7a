#
# GPS and serial interfaces (compiled in kernel)
#
#insmod /system/lib/modules/pl2303.ko
#insmod /system/lib/modules/cp210x.ko
#insmod /system/lib/modules/ftdi_sio.ko
#insmod /system/lib/modules/cdc-acm.ko
